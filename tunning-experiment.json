{
  "variables": [
    {
      "name": "start_date",
      "generator": "date_generator",
      "generator_config": {
        "start": "2019-08-01",
        "end": "2019-12-01"
      }
    }
  ],
  "name": "{start_date}",
  "output": "exp_on_start_date",
  "template": {
    "modeling": {
      "hyper_params": {
        "embedding_size": 256,
        "num_feature_dense": 4,
        "num_dense": 2,
        "bit_loss_weight": 0.7
      },
      "arc_params": {
        "float_features": [
          "map_lat",
          "map_lon",
          "land_front",
          "land_depth",
          "date_end"
        ],
        "num_bits": 32,
        "price_feature_name": "sold_price",
        "bits_feature_name": "bits"
      }
    },
    "training": {
      "batch_size": 1024,
      "epochs": 10000,
      "learning_rate": 0.001,
      "early_stopping": {
        "patience": 20,
        "verbose": 1,
        "mode": "min",
        "restore_best_weights": true
      }
    },
    "datasets": {
      "train": {
        "start": "{start_date}",
        "end": "2019-12-01"
      },
      "dev": {
        "start": "2019-12-01",
        "end": "2020-02-01"
      },
      "test": {
        "start": "2020-02-01",
        "end": "2021-01-01"
      }
    }
  }
}